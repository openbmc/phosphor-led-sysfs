AM_CPPFLAGS = \
	$(PTHREAD_CPPFLAGS) \
	$(SDBUSPLUS_CPPFLAGS) \
	$(PHOSPHOR_DBUS_INTERFACES_CPPFLAGS) \
	$(CODE_COVERAGE_CPPFLAGS)
AM_CFLAGS = \
	$(PTHREAD_CFLAGS) \
	$(SDBUSPLUS_CFLAGS) \
	$(PHOSPHOR_DBUS_INTERFACES_CFLAGS) \
	$(CODE_COVERAGE_CFLAGS)
AM_CXXFLAGS = \
	$(PTHREAD_CXXFLAGS) \
	$(SDBUSPLUS_CXXFLAGS) \
	$(PHOSPHOR_DBUS_INTERFACES_CXXFLAGS) \
	$(CODE_COVERAGE_CXXFLAGS)
AM_LDFLAGS = \
